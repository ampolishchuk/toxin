section.paginator&attributes(attr)
    .paginator-buttons
        if page > 2
            button.paginator-button.previuos
        if page > 3             
            button.paginator-button 1
        if page > 5
            button.paginator-button.dots ...
        if page === 5 
            button.paginator-button 2
        if page > 2
            button.paginator-button #{page - 2}
        if page > 1
            button.paginator-button #{page - 1}

        button.paginator-button.current #{page}

        if total - page > 0
            button.paginator-button #{page + 1}   
        if total - page > 1
            button.paginator-button #{page + 2}
        if total - page === 4 
            button.paginator-button #{total - 1}
        if total - page > 4 
            button.paginator-button.dots ...
        if total - page > 2
            button.paginator-button #{total}
        if total - page > 1
            button.paginator-button.next

    p.paginator-info #{range} вариантов аренды
        


    